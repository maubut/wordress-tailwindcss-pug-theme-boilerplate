{
  "name": "wordress-tailwindcss-pug-theme-boilerplate",
  "version": "1.0.0",
  "main": "index.js",
  "repository": "https://github.com/maubut/wordress-tailwindcss-pug-theme-boilerplate.git",
  "author": "Mikhael Aubut <mikhaelaubut@gmail.com>",
  "license": "MIT",
  "scripts": {
    "webpack": "cross-env NODE_ENV=development webpack --progress --config=node_modules/laravel-mix/setup/webpack.config.js",
    "dev": "cross-env NODE_ENV=development webpack --watch --progress --config=node_modules/laravel-mix/setup/webpack.config.js",
    "hmr": "cross-env NODE_ENV=development webpack-dev-server --hot --config=node_modules/laravel-mix/setup/webpack.config.js",
    "prod": "cross-env NODE_ENV=production webpack --progress --config=node_modules/laravel-mix/setup/webpack.config.js",
    "test": "cross-env nyc ava --verbose --serial",
    "posttest": "cross-env nyc report --reporter=html"
  },
  "dependencies": {
    "@barba/core": "^2.9.7",
    "cross-env": "^7.0.3",
    "laravel-mix": "^6.0.11",
    "postcss-loader": "^5.0.0",
    "postcss-nested": "^5.0.3",
    "tailwindcss": "^2.0.3"
  },
  "devDependencies": {
    "postcss": "8.1",
    "ts-loader": "^8.0.16",
    "typescript": "^4.1.3"
  }
}
